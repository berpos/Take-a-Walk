<h2 class="text-center mt-3">My Bookings</h2>
<div class="countainer row justify-content-center mt-3">
  <div class="col col-5">
    <%@my_bookings.each do |booking|%>
      <div class="booking-card mb-2">
        <%= cl_image_tag booking.walker.photos.first.key %>
        <div class="booking-card-info p-3">
          <p><strong>Booked with <%= "#{booking.walker.first_name} #{booking.walker.last_name}" %></strong></p>
          <p><strong>Booked at <%= booking.start_date %></strong></p>
          <p><strong> to <%= booking.end_date %></strong></p>
          <p><small>congratulations your booking</small></p>
          <span>Â£5.55</span>
        </div>
      </div>
    <% end %>
  </div>
</div>
